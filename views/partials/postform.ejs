            <option value="3">Italian</option>
            <option value="4">Mediterranean</option>
            <option value="5">Latino/Hispanic</option>
            <option value="6">German/Beirgarten</option>
            <option value="7">Indian</option>
            <option value="8">Chinese</option>
            <option value="9">Japanese</option>
            <option value="10">Vegetarian</option>
            <option value="11">Other</option>

          </select>
        </div>
      </div>

      <div class="form-group">
        <label class="col-md-4 control-label" for="textinput">Street</label>  
        <div class="col-md-4">
        <input id="reststreet" name="textinput" type="text" placeholder="Optional" class="form-control input-md">
        </div>
      </div>
  
      <div class="form-group">
        <label class="col-md-4 control-label" for="textinput">City</label>  
        <div class="col-md-4">
        <input id="restcity" name="textinput" type="text" placeholder="Optional" class="form-control input-md">
        </div>
      </div>

      <div class="form-group">
        <label class="col-md-4 control-label" for="textinput">State</label>  
        <div class="col-md-4">
        <input id="reststate" name="textinput" type="text" placeholder="Optional" class="form-control input-md">
        </div>
      </div>
  
      <div class="form-group">
        <label class="col-md-4 control-label" for="textinput">Zip Code</label>  
        <div class="col-md-4">
        <input id="restzip" name="textinput" type="text" placeholder="Required" class="form-control input-md" required="">
        </div>
      </div>
      
      <!-- Textarea -->
      <div class="form-group">
        <label class="col-md-4 control-label" for="textarea">Description</label>
        <div class="col-md-4">                     
          <textarea class="form-control" id="restDescription" name="textarea"></textarea>
          <input type="submit" value="Submit" id="restSubmit">
        </div>
      </div>
      
      </fieldset>
      <legend id="bottomlegend"><button id="gogo">List Your Event</button></legend>  

    </form>